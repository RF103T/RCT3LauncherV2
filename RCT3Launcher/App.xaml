<Application x:Class="RCT3Launcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:local="clr-namespace:RCT3Launcher"
             StartupUri="Views/MainWindow.xaml">
	<Application.Resources>
        <ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/ResourcesDictionary/Languages/zh_CN.xaml"/>
                <ResourceDictionary Source="/ResourcesDictionary/ColorDictionary.xaml"/>
                <ResourceDictionary Source="/ResourcesDictionary/DrawingImageDictionary.xaml"/>
				<ResourceDictionary Source="/ResourcesDictionary/StoryboardDictionary.xaml"/>
				<ResourceDictionary Source="/ResourcesDictionary/StyleDictionary.xaml"/>
			</ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style x:Key="hyberlink" TargetType="{x:Type Hyperlink}">
		<Setter Property="Foreground" Value="{StaticResource Theme.Primary.Normal.SolidColorBrush}"></Setter>
		<Setter Property="TextBlock.TextDecorations" Value="{x:Null}"></Setter>
		<Style.Triggers>
			<Trigger Property="IsMouseOver" Value="True">
				<Setter Property="Foreground" Value="{StaticResource Theme.Primary.Light.SolidColorBrush}"></Setter>
			</Trigger>
		</Style.Triggers>
	</Style>
	<Style TargetType="Hyperlink" BasedOn="{StaticResource hyberlink}"/>
</ResourceDictionary>
<Application x:Class="RCT3Launcher.Updater.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RCT3Launcher.Updater"
             StartupUri="MainWindow.xaml"
			 Startup="OnStartup">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="pack://application:,,,/zh_CN.xaml"/>
				<ResourceDictionary Source="pack://application:,,,/ColorDictionary.xaml"/>
				<ResourceDictionary Source="pack://application:,,,/BrushDictionary.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
